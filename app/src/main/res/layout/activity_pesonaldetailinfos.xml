                android:layout_height="@dimen/dp_30"
                android:gravity="center_vertical"
                android:paddingStart="@dimen/dp_20"
                android:text="定位信息"
                android:textColor="#323232"
                android:textSize="@dimen/sp_12"
                tools:ignore="HardcodedText,RtlSymmetry"/>

            <TextView
