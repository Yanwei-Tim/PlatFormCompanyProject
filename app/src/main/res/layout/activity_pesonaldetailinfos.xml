                style="@style/tv_start20_ts14_tcbla"
                android:layout_below="@id/tv_location"
                android:text="时间"
                tools:ignore="HardcodedText,RtlSymmetry"/>

            <TextView
                android:id="@+id/tv_time"
                style="@style/tv_end20_ts14"
                android:layout_below="@id/tv_location"
                android:layout_toEndOf="@id/time"
