<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_bg_logo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg"
        android:gravity="center"
        android:paddingTop="@dimen/dp_24"
        android:text="每到一城，美一城"
        android:textColor="@color/white_ed"
        android:textSize="@dimen/sp_16"
        tools:ignore="HardcodedText"/>


    <ScrollView
        android:id="@+id/scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="UselessParent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical"
            tools:ignore="ScrollViewSize">

            <TextView
                style="@style/Height_40_bg_gray_hand_txtcolor_hand_gray"
                android:text="@string/quality_manage"
                tools:ignore="RtlSymmetry"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@color/white"
                android:orientation="horizontal"
                tools:ignore="UselessLeaf">

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_problemupload"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_problemupload"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/zuoyejiancha"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_problemupload"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="作业监察"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>


                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_qualityxuncha"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_qualityxuncha"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/wentichuli"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_qualityxuncha"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="问题处理"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_operationweigui"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_operationweigui"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/dingweixinxi"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_operationweigui"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="定位信息"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_kaoqin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_kaoqin"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/zuoyekaoqin"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_kaoqin"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="作业考勤"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

            </LinearLayout>

            <TextView
                style="@style/Height_40_bg_gray_hand_txtcolor_hand_gray"
                android:text="@string/operation_manage"
                tools:ignore="RtlSymmetry"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@color/white"
                android:orientation="horizontal"
                android:visibility="visible"
                tools:ignore="UselessLeaf">

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_msgcollection"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_msgcollection"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/xinxicaiji"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_msgcollection"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="信息采集"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_historytrack"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_historytrack"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/lishiguiji"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_historytrack"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="历史轨迹"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_operationbianji"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_operationbianji"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/weiguizuoye"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_operationbianji"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="违规作业"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_videojian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_videojian"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/shipinjiankong"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_videojian"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="视频监控"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@color/white"
                android:orientation="horizontal"
                android:visibility="visible"
                tools:ignore="UselessLeaf">

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_msgcollection1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_msgcollection1"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/zuoyekaoqin"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_msgcollection1"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="主管考勤"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_historytrack2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector"
                    android:visibility="invisible">

                    <ImageView
                        android:id="@+id/iv_historytrack2"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/lishiguiji"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_historytrack2"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="历史轨迹"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_operationbianji3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector"
                    android:visibility="invisible">

                    <ImageView
                        android:id="@+id/iv_operationbianji3"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/weiguizuoye"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_operationbianji3"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="违规作业"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_videojian4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector"
                    android:visibility="invisible">

                    <ImageView
                        android:id="@+id/iv_videojian4"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/shipinjiankong"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_videojian4"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="视频监控"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

            </LinearLayout>

            <TextView
                style="@style/Height_40_bg_gray_hand_txtcolor_hand_gray"
                android:text="资产管理"
                tools:ignore="HardcodedText,RtlSymmetry"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@color/white"
                android:orientation="horizontal"
                android:visibility="visible"
                tools:ignore="UselessLeaf">

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_money_manage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_money_manage"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_xinzengshenqing"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_money_manage"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="新增申请"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_historytrack1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector">

                    <ImageView
                        android:id="@+id/iv_historytrack1"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_wodeshenqing"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_historytrack1"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="我的申请"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_operationbianji2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector"
                    android:visibility="invisible">

                    <ImageView
                        android:id="@+id/iv_operationbianji2"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/weiguizuoye"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_operationbianji2"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="违规作业"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

                <RelativeLayout
                    android:id="@+id/rl_videojian2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/ll_customer_selector"
                    android:visibility="invisible">

                    <ImageView
                        android:id="@+id/iv_videojian2"
                        style="@style/iv_pading_10"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/shipinjiankong"
                        tools:ignore="ContentDescription"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_videojian2"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="视频监控"
                        android:textColor="@color/gray_1f"
                        android:textSize="@dimen/sp_12"
                        tools:ignore="HardcodedText"/>

                </RelativeLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/white"/>

            </LinearLayout>

            <TextView
                style="@style/Height_40_bg_gray_hand_txtcolor_hand_gray"
                android:text="@string/msg_tixing"
                android:visibility="invisible"
                tools:ignore="RtlSymmetry"/>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/recyclerview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>


        </LinearLayout>

    </ScrollView>

</LinearLayout>